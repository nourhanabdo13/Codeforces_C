#include <stdio.h>
#include <math.h>
unsigned int i = 0;
unsigned int j = 0;
unsigned int integer = 0;
unsigned int out = 0;
int main()
{
    for(i = 1;i <= 5;i++)
    {
        for(j = 1;j <= 5;j++)
        {
            scanf("%i",&integer);
            if(integer == 1)
            {
                out = abs(i-3) + abs(j-3);
            }
        }
    }
    printf("%i",out);
    return 0;
}
