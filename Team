#include <stdio.h>
unsigned int n = 0;
unsigned int p = 0;
unsigned int v = 0;
unsigned int t = 0;
unsigned int out_pro = 0;
unsigned int i = 0;
int main()
{
    scanf("%i",&n);
    for(i = 1;i <= n;i++)
    {
        scanf("%i%i%i",&p,&v,&t);
        if((p==1&&v==1&&t==1)||(p==0&&v==1&&t==1)||(p==1&&v==0&&t==1)||(p==1&&v==1&&t==0))
        {
            out_pro = out_pro + 1;
        }
    }
    printf("%i",out_pro);
 
    return 0;
}
